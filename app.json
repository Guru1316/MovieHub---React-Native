{
  "expo": {
    "name": "MovieHub",
    "slug": "MovieHubApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/MovieHub.png",
    "scheme": "moviehubapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#121212",
        "foregroundImage": "./app/assets/MovieHub.png"
      },
      "package": "com.guru1613.moviehub"
    },
    "web": {
      "output": "static",
      "favicon": "./app/assets/MovieHub.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./app/assets/MovieHub.png",
          "resizeMode": "contain",
          "backgroundColor": "#121212"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3f0bb757-67c4-40fa-bbfe-60e3df4560f3"
      }
    }
  }
}